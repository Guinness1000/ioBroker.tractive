{
  "name": "iobroker.tractive",
  "version": "0.0.1",
  "description": "Adapter for Tractive GPS Tracker",
  "author": {
    "name": "Patrick Winkler",
    "email": "patrick.winkler@web.de"
  },
  "homepage": "https://github.com/Guinness1000/ioBroker.tractive",
  "license": "MIT",
  "keywords": [
    "Tractive",
    "GPS"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Guinness1000/ioBroker.tractive"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^2.2.2",
    "@iobroker/adapter-dev": "^1.0.1",
    "@iobroker/testing": "^3.0.2",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.24",
    "@types/node-fetch": "^2.6.2",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.22.0",
    "mocha": "^10.0.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "typescript": "~4.5.5"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "lib/",
    "widgets/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint",
    "translate": "translate-adapter",
    "release": "release-script patch --yes",
    "release-minor": "release-script minor --yes",
    "release-major": "release-script major --yes"
  },
  "bugs": {
    "url": "https://github.com/Guinness1000/ioBroker.tractive/issues"
  },
  "readmeFilename": "README.md"
}
